js OPERATORS--------------------------------------->
var a=2
var b=3
var c=a+b;
console.log(c)----->5 

var a=10;
a++; or ++a;
console.log(a)   ----->11


var a=10;
a--; or --a;
console.log(a)------>9

var a=10
var b=a++ or b=++a
console.log(b)----->10   or 11

var a=10
var b=--a or b=a--
console.log(b)----->9 or 10




dataTypes--------------------------------------------->

console.log(10);--->10
console.log("star")---->star
console.log(typeof(10))----->number
console.log(typeof(star))------->undefined
console.log(typeof("star"))------>string

var a=null
console.log(a)----->null

var a=null
console.log(typeof(a))------>boolean



